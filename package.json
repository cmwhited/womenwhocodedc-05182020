{
  "name": "womenwhocodedc-05182020",
  "version": "1.0.0",
  "description": "Serverless webpack example using Typescript",
  "main": "handler.js",
  "scripts": {
    "check-types": "tsc",
    "lint:eslint": "eslint --ext .ts . --ignore-path .gitignore",
    "format:eslint": "eslint --ext .ts . --fix --ignore-path .gitignore",
    "lint:prettier": "prettier \"**/*.ts\" --list-different --ignore-path .gitignore || (echo '↑↑ these files are not prettier formatted ↑↑' && exit 1)",
    "format:prettier": "prettier \"**/*.ts\" --write --ignore-path .gitignore",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "clean-report": "rimraf ./coverage",
    "test": "npm run clean-report && jest -c jest.config.js --runInBand --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "prebuild": "rimraf ./lib",
    "build": "tsc && tsc -m es6 --outDir lib-esm",
    "local:offline": "sls offline start --port 8080",
    "deploy": "sls deploy",
    "remove": "sls remove"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.6",
    "@types/pg": "^7.14.3",
    "apollo-server-lambda": "^2.13.1",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.679.0",
    "dataloader": "^2.0.0",
    "graphql": "^15.0.0",
    "graphql-tools": "^5.0.0",
    "pg": "^8.2.1",
    "source-map-support": "^0.5.10"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-destructuring": "^7.9.5",
    "@babel/plugin-transform-regenerator": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@types/aws-lambda": "^8.10.51",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.1",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.1",
    "eslint-plugin-prettier": "^3.1.3",
    "fork-ts-checker-webpack-plugin": "^4.1.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "serverless": "^1.71.1",
    "serverless-dotenv-plugin": "^2.4.2",
    "serverless-offline": "^6.1.5",
    "serverless-webpack": "^5.3.2",
    "ts-jest": "^26.0.0",
    "ts-loader": "^7.0.4",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-node-externals": "^1.7.2"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
